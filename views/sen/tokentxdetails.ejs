<div class="wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 col-xs-12">
        <ol class="breadcrumb m-0 p-0">
          <li class="breadcrumb-item"><a href="/sen/tokenblockchain">SEN Blocks</a></li>
          <li class="breadcrumb-item active">SEN Transaction Info</li>
        </ol>
      </div>
    </div>
    <div class="row m-t-20">
      <div class="col-lg-12">
        <div class="card-box">
          <% for (key in transaction) { %>
          <% if (key !== "TxReceiptStatus" && key.substring(0, 3) !== "Gas") { %>
          <div class="cardPt2">
            <label class="publicListTit tradingListTit text-left"><%= key === "BlockNumber" ? "Block Height" : key %>: </label>
            <span class="publicListTxt tradingListTxt" id="hashValues"><%= typeof transaction[key] === 'object' ? JSON.stringify(transaction[key]) : key === 'TimeStamp' ? new Date(transaction[key]).toLocaleString() : key === 'TxHash' || key === 'TxFrom' || key === 'TxTo' ? '0x' + transaction[key] : transaction[key] %></span>
          </div>
          <% } %>
          <% } %>
        </div>
      </div>
    </div>
  </div> <!-- end container -->
</div>
<!-- end wrapper -->