<div class="wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 col-xs-12">
        <ol class="breadcrumb m-0 p-0">
          <li class="breadcrumb-item"><a href="/tokenblockchain">Token
              Blockchain</a></li>
          <li class="breadcrumb-item active">Node Informations</li>
        </ol>
      </div>
    </div>
    <div class="row m-t-20">
      <div class="col-lg-12">
        <div class="card-box">
          <h4 class="header-title">Current Nodes <span id="currentNode"><%= nodes.length %></span></h4>
          <div id="world-map-markers" style="height: 500px"></div>
        </div>
      </div>
    </div>
    <div class="card-box">
      <h4 class="header-title">SEC-Blockchain Nodes List</h4>
      <div class="table-responsive" style="max-height: 500px; overflow-y: auto">
        <table class="table table-hover table-bordered table-striped">
          <thead>
            <tr>
              <th>Node Hash</th>
              <th>Node IP Address</th>
              <th>Node Country</th>
              <th>Node City</th>
              <th>Node Timezone</th>
            </tr>
          </thead>
          <tbody id="nodes-table"></tbody>
        </table>
        <div id="pagination" class="pagination pagination-split justify-content-end footable-pagination m-t-10">
        </div>
      </div>
    </div>
  </div>
</div> <!-- end container -->

        