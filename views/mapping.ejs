<div class="wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12">
        <h3>BIUTBlock - Mapping</h3>
        <form class="form-horizontal" role="form" method="post">
          <div class="form-group row">
            <label class="col-2 col-form-label">ETH Private Key</label>
            <div class="col-md-10">
              <input class="form-control" type="text" name="ethprivatekey"
                value="<%= mapping.ethprivatekey %>" required>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-2 col-form-label">BIU Address</label>
            <div class="col-md-10">
              <input class="form-control" type="text" name="biutaddress" value="<%=
                mapping.biutaddress %>" required>
            </div>
          </div>
          <div class="form-group row">
            <label class="col-2 col-form-label">Value</label>
            <div class="col-md-10">
              <input class="form-control" type="text" name="value" value="<%=
                mapping.value %>">
            </div>
          </div>
          <button type="submit" class="btn btn-primary pull-right">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>
