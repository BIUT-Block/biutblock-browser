<div class="wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 col-xs-12">
        <ol class="breadcrumb m-0 p-0">
          <li class="breadcrumb-item"><a href="/tokenblockchain">Token Blockchain</a></li>
          <li class="breadcrumb-item active">Block</li>
        </ol>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="m-t-20 card-box">
          <div class="p-t-10">
            <label class="publicListTit text-left">Hash: </label>
            <span class="publicListTxt" id="hashValues"><%= block.Hash %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Height: </label>
            <span class="publicListTxt" id="heightBlock"><%= block.Number %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Time Stamp:</label>
            <span class="publicListTxt" id="time"><%= block.TimeStamp %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Parent Hash:</label>
            <span class="publicListTxt" id="fatherHash"><%= block.ParentHash %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Difficulty:</label>
            <span class="publicListTxt" id="difficulty"><%= block.Difficulty %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Gas Used:</label>
            <span class="publicListTxt" id="gasUsed"><%= block.GasUsed %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Gas Limit:</label>
            <span class="publicListTxt" id="gasLimit"><%= block.GasLimit %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Extra Data:</label>
            <span class="publicListTxt" id="extraData"><%= block.ExtraData %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Transactions Root: </label>
            <span class="publicListTxt" id="transactionsRoot">0x<%= block.TransactionsRoot %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Receipt Root:</label>
            <span class="publicListTxt" id="receiptRoot">0x<%= block.ReceiptRoot %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Logs Bloom:</label>
            <span class="publicListTxt" id="logsBloom"><%= block.LogsBloom %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Mix Hash:</label>
            <span class="publicListTxt" id="mixHash">0x<%= block.MixHash %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">State Root:</label>
            <span class="publicListTxt" id="stateRoot">0x<%= block.StateRoot %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Nonce:</label>
            <span class="publicListTxt" id="nonce"><%= block.Nonce %></span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Block Reward:</label>
            <span class="publicListTxt" id="reward">0 SEC</span>
          </div>
          <div class="p-t-10">
            <label class="publicListTit text-left">Beneficiary:</label>
            <span class="publicListTxt" id="beneficiary">0x<%= block.Beneficiary %></span>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12 col-xs-12">
        <div class="card-box">
          <h4 class="header-title">Transactions Sum <span id="total"><%= block.Transactions.length %></span></h4>
          <div class="table-responsive">
            <table class="table table-hover table-bordered">
              <thead>
                <tr>
                  <th>TxHash</th>
                  <th>From</th>
                  <th>To</th>
                  <th>Value</th>
                  <th>Status</th>
                </tr>
              </thead>
              <tbody>
                <% block.Transactions.forEach((Tx) => { %>
                <tr>
                  <td><a href="/tokentxdetails?hash=<%= Tx.TxHash %>">0x<%= Tx.TxHash %></a></td>
                  <td><a href="/accountdetails?address=<%= Tx.TxFrom %>">0x<%= Tx.TxFrom %></a></td>
                  <td><a href="/accountdetails?address=<%= Tx.TxFrom %>">0x<%= Tx.TxTo %></a></td>
                  <td><%= Tx.Value %></td>
                  <td><%= Tx.TxReceiptStatus %></td>
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div> <!-- end container -->
</div>
<!-- end wrapper -->