<div class="wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-md-12 col-xs-12">
        <ol class="breadcrumb m-0 p-0">
          <li class="breadcrumb-item"><a href="/tokenblockchain">Token Blockchain</a></li>
          <li class="breadcrumb-item active">Token Tx List</li>
        </ol>
      </div>
    </div>
    <div class="row m-t-20">
      <div class="col-md-12 col-xs-12">
        <div class="card-box">
          <h4 class="header-title">Transactions Sum <span id="total"><%= totalNumber %></span></h4>
          <div class="table-responsive">
            <table class="table table-hover table-bordered">
              <thead>
                <tr>
                  <th>TxHash</th>
                  <th>Block Number</th>
                  <th>From</th>
                  <th>To</th>
                  <th>Value</th>
                  <th>Time Stamp</th>
                  <th>Status</th>
                </tr>
              </thead>
              <tbody id="tx-table">
                <% transactions.forEach(tx=>{ %>
                <tr>
                  <td><a href="/tokentxdetails?hash=<%= tx.TxHash %>">0x<%= tx.TxHash %></a></td>
                  <td><a href="/tokenblockdetailsbynumber?number=<%= tx.BlockNumber %>"><%= tx.BlockNumber %></a></td>
                  <td><a href="/accountdetails?address=<%= tx.TxFrom %>">0x<%= tx.TxFrom %></a></td>
                  <td><a href="/accountdetails?address=<%= tx.TxTo %>">0x<%= tx.TxTo %></a></td>
                  <td><%= tx.Value %> SEC</td>
                  <td><%= tx.TimeStamp %></td>
                  <td><%= tx.TxReceiptStatus %></td>
                </tr>
                <% }) %>
              </tbody>
            </table>
            <div class="page" id="pagination">
            </div>
          </div>
        </div>
      </div>
    </div>

  </div> <!-- end container -->
</div>
<!-- end wrapper -->
<script>
var totalNumber = <%- totalNumber %>
</script>